@{
  Layout="Shared/_Layout.cshtml";
}

<h3>Clients</h3>
<ul>
  @if (Model.Count !=0)
  {
    @foreach(var client in Model)
    {
      <li><a href="/clients/@client.GetId()">@client.GetName()</a></li>
    }
  }
  else
  {
    <p>AINT NOTHIN HERE</p>
  }
</ul>
<h4><a href ="/clients/new">Add a new client!</a></h4>
<h4><a href="/">Return home</a></h4>
